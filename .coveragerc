[run]
source = src
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */venv/*
    */env/*
    */ENV/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Address Cleanser Coverage Report

[xml]
output = coverage.xml
